{
  "name": "@agentlogs/web",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bash ../../scripts/check-and-migrate.sh && bun run cf-typegen && npx @react-grab/claude-code@latest && vite dev",
    "dev:test": "VITE_USE_TEST_DB=true bun run cf-typegen && vite dev",
    "dev:wrangler": "wrangler dev --local --log-level=debug",
    "build": "vite build",
    "preview": "vite preview",
    "predeploy": "bun run cf-typegen && bun run build",
    "deploy": "wrangler deploy",
    "cf-typegen": "test -f .env || cp .env.example .env; wrangler types",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply agentlogs --local",
    "db:migrate:remote": "wrangler d1 migrations apply agentlogs --remote",
    "db:studio": "DB_LOCAL_PATH=$(find $PWD/.wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit 2>/dev/null) bunx drizzle-kit studio --port 4983",
    "db:studio:test": "DB_LOCAL_PATH=$(find $PWD/.wrangler-test/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit 2>/dev/null) bunx drizzle-kit studio --port 4984",
    "db:reset": "rm -rf .wrangler/state && bun run db:migrate",
    "check": "tsgo --project tsconfig.json"
  },
  "dependencies": {
    "@agentlogs/shared": "workspace:*",
    "@base-ui/react": "^1.1.0",
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource/instrument-serif": "^5.2.8",
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "@paralleldrive/cuid2": "^2.3.0",
    "@pierre/diffs": "^1.0.4",
    "@sentry/cloudflare": "^10.34.0",
    "@sentry/tanstackstart-react": "^10.34.0",
    "@tanstack/react-router": "^1.145.11",
    "@tanstack/react-start": "^1.145.11",
    "ai": "^6.0.35",
    "better-auth": "1.4.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.6",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.546.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "shadcn": "^3.7.0",
    "streamdown": "^2.0.1",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0",
    "vite": "^7.1.9"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.27",
    "@cloudflare/vite-plugin": "^1.13.13",
    "@sentry/vite-plugin": "^4.6.2",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "drizzle-kit": "^0.31.5",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.6.2",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.43.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@latest"
  }
}
