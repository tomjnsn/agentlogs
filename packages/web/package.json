{
  "name": "@vibeinsights/web",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bash ../../scripts/check-and-migrate.sh && bun run cf-typegen && vite dev",
    "dev:test": "VITE_USE_TEST_DB=true bun run cf-typegen && vite dev",
    "build": "vite build && tsc --noEmit",
    "preview": "vite preview",
    "predeploy": "bun run cf-typegen && bun run build",
    "deploy": "wrangler deploy",
    "cf-typegen": "test -f .env || cp .env.example .env; wrangler types",
    "db:setup": "bun run db:generate && bun run db:migrate:local",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply vibeinsights --local",
    "db:migrate:remote": "wrangler d1 migrations apply vibeinsights --remote",
    "db:studio": "DB_LOCAL_PATH=$(find $PWD/.wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit 2>/dev/null) bunx drizzle-kit studio --port 4983",
    "db:studio:test": "DB_LOCAL_PATH=$(find $PWD/.wrangler-test/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit 2>/dev/null) bunx drizzle-kit studio --port 4984",
    "db:reset": "rm -rf .wrangler/state && bun run db:setup",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-start": "^1.132.56",
    "@vibeinsights/shared": "workspace:*",
    "better-auth": "^1.3.27",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.6",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.545.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "vite": "^7.1.9"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.27",
    "@cloudflare/vite-plugin": "^1.13.12",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "drizzle-kit": "^0.31.5",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.6.2",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.42.2"
  }
}
