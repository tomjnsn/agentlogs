{
  "name": "@vibeinsights/cli",
  "version": "0.0.5",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "vibeinsights": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "bun build ./src/index.ts --outdir ./dist --target node --external @napi-rs/keyring --production --define '__NODE_ENV__=\"production\"' --define 'process.env.SERVER_URL=\"https://vibeinsights.dev\"'",
    "prepublishOnly": "bun run build",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "dependencies": {
    "@napi-rs/keyring": "^1.2.0"
  },
  "devDependencies": {
    "@vibeinsights/shared": "workspace:*",
    "better-auth": "^1.3.27",
    "open": "^10.2.0"
  }
}
