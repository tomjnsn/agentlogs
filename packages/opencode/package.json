{
  "name": "@agentlogs/opencode",
  "version": "0.0.1",
  "keywords": [
    "agentlogs",
    "ai-coding",
    "opencode",
    "plugin",
    "transcript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentlogs/agentlogs.git",
    "directory": "packages/opencode"
  },
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node --external @agentlogs/shared",
    "prepublishOnly": "bun run build",
    "check": "tsgo --project tsconfig.json"
  },
  "dependencies": {
    "@agentlogs/shared": "workspace:*"
  },
  "peerDependencies": {
    "bun": ">=1.0.0"
  }
}
